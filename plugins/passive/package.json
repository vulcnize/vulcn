{
  "name": "@vulcn/plugin-passive",
  "version": "0.1.0",
  "description": "Passive security scanner for Vulcn â€” detects missing security headers, insecure cookies, information disclosure, CORS misconfigurations, and mixed content without injecting payloads.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "vulcn",
    "plugin",
    "passive-scan",
    "security-headers",
    "cookie-security",
    "information-disclosure",
    "cors",
    "mixed-content",
    "csp",
    "hsts",
    "security-testing",
    "penetration-testing",
    "web-security",
    "owasp",
    "dast"
  ],
  "author": "rawlab",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vulcnize/vulcn.git",
    "directory": "plugins/passive"
  },
  "homepage": "https://docs.vulcn.dev",
  "bugs": {
    "url": "https://github.com/vulcnize/vulcn/issues"
  },
  "peerDependencies": {
    "@vulcn/engine": ">=0.4.0"
  },
  "dependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vulcn/engine": "workspace:*",
    "playwright": "^1.50.0",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0"
  }
}
