{
  "name": "@vulcn/plugin-detect-xss",
  "version": "0.2.2",
  "description": "Execution-based XSS detection plugin for Vulcn â€” detects real JavaScript execution (alert, confirm, console) rather than pattern matching, for accurate cross-site scripting vulnerability identification.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "vulcn",
    "plugin",
    "xss",
    "xss-detection",
    "cross-site-scripting",
    "vulnerability-detection",
    "security-testing",
    "penetration-testing",
    "javascript-injection",
    "owasp",
    "owasp-top-10",
    "web-security",
    "appsec",
    "dast"
  ],
  "author": "rawlab",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vulcnize/vulcn.git",
    "directory": "plugins/detect-xss"
  },
  "homepage": "https://docs.vulcn.dev",
  "bugs": {
    "url": "https://github.com/vulcnize/vulcn/issues"
  },
  "peerDependencies": {
    "@vulcn/engine": ">=0.3.0"
  },
  "dependencies": {
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vulcn/engine": "workspace:*",
    "playwright": "^1.50.0",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0"
  }
}
