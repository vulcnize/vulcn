{
  "name": "@vulcn/plugin-detect-xss",
  "version": "0.1.0",
  "description": "Execution-based XSS detection plugin for Vulcn - detects actual JavaScript execution",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "vulcn",
    "plugin",
    "xss",
    "detection",
    "security",
    "testing",
    "execution"
  ],
  "author": "rawlab",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vulcnize/vulcn.git",
    "directory": "plugins/detect-xss"
  },
  "peerDependencies": {
    "@vulcn/engine": ">=0.2.0"
  },
  "dependencies": {
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vulcn/engine": "workspace:*",
    "playwright": "^1.50.0",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0"
  }
}
