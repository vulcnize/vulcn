{
  "name": "@vulcn/driver-browser",
  "version": "1.0.0",
  "description": "Browser recording driver for Vulcn - Record and replay web application interactions using Playwright",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "vulcn",
    "driver",
    "browser",
    "playwright",
    "recording",
    "security",
    "testing",
    "web"
  ],
  "author": "rawlab",
  "license": "AGPL-3.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vulcnize/vulcn.git",
    "directory": "drivers/browser"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@vulcn/engine": ">=0.3.0"
  },
  "dependencies": {
    "playwright": "^1.50.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@vulcn/engine": "workspace:*",
    "tsup": "^8.4.0",
    "typescript": "^5.7.0"
  }
}
